# -*- mode: snippet -*-
# name: Browser.sandbox
# --
module Main exposing (..)

import Browser
import Html exposing (Html, text)

main =
  Browser.sandbox
    { init = init, update = update, view = view }

type alias Model = Int

init : Model
init = 0

view : model -> Html msg
view model = text "sandbox"

update : msg -> model -> model
update model = model